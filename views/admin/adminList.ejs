<% include header %>
	<!-- 路径导航 -->
	<ol class="breadcrumb">
	  <li><a href="#">用户管理</a></li>
	  <li class="active">管理员列表</li>
	</ol>
	<!-- 路径导航 end -->

	<!-- 数据列表 -->
	<div class="row">
		<div class="col-md-12">
			<table class="table table-hover articleList">
				<tr>
					<th>用户账号</th>
					<th>用户简介</th>
					<th>联系电话</th>
					<th>创建时间</th>
					<th>操作</th>
				</tr>
				<tr>
				<% admins.forEach(function(val){ %>
					<td><%= val.name %></td>
					<td><%= val.info %></td>
					<td><%= val.tel %></td>
					<td><%= val.ctime.toLocaleString() %></td>
					<td>
						<a href="/admin/adminEdit/<%=val._id%>">编辑</a>
						|
						<a href="/admin/adminRemove/<%=val._id%>" onclick="return confirm('您确定要删除吗?')">
								删除
						</a>
					</td>
				</tr>
				<% })%>
			</table>
		</div>
	</div>

	
<% include footer %>
