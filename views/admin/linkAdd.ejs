<!-- 引入 公用的头部 模版 -->
<% include header%>

<div class="container">
	<!-- 路径导航 -->
	<ol class="breadcrumb min-nav-bg">
	  <li><a href="#">链接管理</a></li>
	  <li class="active">添加链接</li>
	</ol>
	<!-- 路径导航 end -->

	<!-- 添加数据的表单 -->
	<div class="row">
		<div class="col-md-4 col-sm-2 col-xs-0"></div>
		<div class="col-md-4 col-sm-8 col-xs-12">
			<form class="formAdd" action="/admin/linkInsert" method="post">
				<div class="input-group">
				  <span class="input-group-addon">链接名称</span>
				  <input type="text" class="form-control" name="name">
				</div>
				
				<div class="input-group">
				  <span class="input-group-addon">链接地址</span>
				  <input type="text" class="form-control" name="url">
				</div>

				<div class="input-group">
				  <span class="input-group-addon">栏目排序</span>
				  <input type="number" class="form-control" name="order">
				</div>

				<div class="input-group nav-sub">
					<input class="nav-submit" id="btn-submit"  type="submit" value="提交">
				</div>
					
			</form>
		</div>
		<div class="col-md-4 col-sm-2 col-xs-0"></div>
	</div>
</div>



<!-- 引入 公用的底部 模版 -->
<% include footer%>
